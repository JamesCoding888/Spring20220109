<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">
	
	
	<bean id="testBean1" scope="prototype" class="com.study.spring2.case02.TestBean">
		<property name="id">
			<value>2</value>
		</property>
		
		<property name="name">
			<value>David</value>
		</property>
		
		<property name="address">
			<value>Taipei</value>
		</property>

	</bean>
  
	<bean id="testBean2" scope="prototype"
		class="com.study.spring2.case02.TestBean">
		<property name="id">
			<value>3</value>
		</property>

		<property name="name">
			<value>John</value>
		</property>

		<property name="address">
			<value>Taichu</value>
		</property>
		
	</bean>	

